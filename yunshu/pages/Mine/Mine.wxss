page{
  width:100%;
}
.container
{
  width:100%;
}
.wrapper
{
  height:300rpx;
  display: flex;
  width:710rpx;
  padding:30rpx 20rpx 20rpx 20rpx;
  background: #1984D1;
  justify-content: space-between;
}
.impress
{
  padding-top:10rpx;
}
.impress image
{
  width:180rpx;
  height:180rpx;
  border-radius:50%; 
  padding:5rpx;
}
.wrapper .descSelf
{ 
  width:100%;
  margin-left:30rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.descSelf .name
{
  font-size:16px;
  color:#fff
}
.descSelf .infomation
{
  font-size:12px;
  color:#fff
} 
.collectionNotic
{ 
  padding-top:30rpx;
  width:80%;
  display: flex;
  justify-content: space-between;
  font-size:12px;
  color:#fff
}
.collectionNotic .collection
{
  display: flex;
  flex-direction: column;
  align-items: center
}
.collectionNotic .notic
{
  display: flex;
  flex-direction: column;
  align-items: center
}
.loading
{
  position: fixed;
  left:50%;
  top:50%;
  background: #fff;
  width:200rpx;
  height: 200rpx; 
  margin: -100rpx; 
}